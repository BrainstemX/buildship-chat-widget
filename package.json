  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "source": "src/index.ts",
  "types": "dist/src/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
